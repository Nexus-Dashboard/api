{
  "name": "api-nexus",
  "version": "1.0.0",
  "description": "Sistema de análise de pesquisas com autenticação",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "create-admin": "node scripts/create-admin.js",
    "bq:check": "node scripts/check-mongodb-data.js",
    "bq:setup": "node scripts/setup-bigquery-tables.js",
    "bq:migrate": "node scripts/migrate-test-data.js",
    "bq:migrate-all": "node scripts/migrate-full-data.js",
    "bq:test": "node scripts/test-bigquery-queries.js",
    "bq:cleanup": "node scripts/cleanup-bigquery-test.js",
    "bq:costs": "node scripts/test-bigquery-queries.js costs",
    "migrate:sheets": "node scripts/migrate-from-google-sheets.js",
    "migrate:sheets:telephonic": "node scripts/migrate-from-google-sheets.js",
    "migrate:sheets:f2f": "node scripts/migrate-from-google-sheets.js --f2f",
    "migrate:sheets:dry-run": "node scripts/migrate-from-google-sheets.js --dry-run",
    "migrate:sheets:f2f:dry-run": "node scripts/migrate-from-google-sheets.js --f2f --dry-run",
    "test:google-drive": "node scripts/test-google-drive-access.js",
    "migrate:cluster": "node scripts/migrate-cluster-telephonic.js",
    "copy:users": "node scripts/copy-users-to-f2f.js",

    "start:bigquery": "USE_BIGQUERY=true node server.js",
    "start:hybrid": "USE_BIGQUERY=true BIGQUERY_FALLBACK=true node server.js"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^8.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "googleapis": "^126.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
